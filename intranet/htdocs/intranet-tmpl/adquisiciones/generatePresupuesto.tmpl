<fieldset id="datos_proveedor">
<legend class="titulo_legend">[% "Proveedores a Presupuestar" | i18n %]</legend>
    <ul>
        <li class="sub_item">Proveedor/es:</li>
        <li class="sub_item"> [% combo_proveedores %] </li>
    </ul>
    <ul>
        [% PERL %]
            print C4::AR::Filtros::to_Button( 
                                                text    =>  "[% 'Aceptar' | i18n %]",
                                                boton   =>  "boton_buscar",
                                                onClick =>  "generatePresupuesto([% pedido_cotizacion_id %]);",
                    ) ;
            print C4::AR::Filtros::to_Button( 
                                                text    =>  "[% 'Exportar' | i18n %]",
                                                boton   =>  "boton_buscar",
                                                onClick =>  "exportar([% pedido_cotizacion_id %])",
                    ) ;
        [% END %]  
        [% PERL %]
                                            print C4::AR::Filtros::link_to(   
                                                                            text =>     C4::AR::Filtros::to_Icon( boton   => "icon_mas" ),
                                                                            url =>      "/cgi-bin/koha/adquisiciones/export_temp.pl", 
                                                                            params =>   ["pedido_cotizacion_id=[% pedido_cotizacion_id %]",
                                                                                        "proveedor=2"],
                                                                            title =>    "[% 'Exportar' | i18n %]"
                                                                        );
                              [% END %]
    </ul>
</fieldset> 

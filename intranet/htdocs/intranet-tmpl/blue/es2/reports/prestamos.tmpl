<script language="JavaScript">

function hacerSubmit(){
        document.branches.submit();
        }

function hacerSubmit(frm,accion){
			if (accion == 'search'){frm.action='/cgi-bin/koha/reports/prestamos.pl'}
			else {frm.action='/cgi-bin/koha/reports/mailvencidos.pl'}
			frm.submit();
}


function checkedAll(){
	var checks=document.getElementsByTagName("input");
	if (checks.length>0){
		for(i=0;i<checks.length;i++){
			if(checks[i].type == "checkbox"){
				if(checks[i].checked){checks[i].checked=false}
                   	 	else{checks[i].checked=true}
			}
		}
	}
}

</script>


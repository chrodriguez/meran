
 <script src="/includes/jquery/ui/tabs.min.js" type="text/javascript"></script>
  <link media="screen" rel="stylesheet" href="/includes/jquery/ui/base/jquery.ui.tabs.css" type="text/css">

<script>
    $(function() {
        $( "#tabs" ).tabs();
    });
</script>

<fieldset class="resultados_busqueda">
    <legend class="titulo_legend trigger click">[% "Detalle de Registro" | i18n %]</legend>
    <div class="detalleMARC">
    <div id="tabs" class="ui-tabs ui-widget ui-widget-content ui-corner-all">
        <ul>
            <li><a href="#tabs-1">Registro Original</a></li>
            <li><a href="#tabs-2">Registro Resultado</a></li>
        </ul>
        <div id="tabs-1" >
            <br><br>

            <fieldset class="resultados_busqueda">
            <legend class="titulo_legend trigger click">[% "Registro" | i18n %]</legend>
                <pre>
                    [% registro_importacion.getRegistroMARCOriginal.as_formatted %]
                </pre>

                [% FOREACH registro_ejemplar IN registro_importacion.getRegistrosHijo %]

                    <fieldset class="resultados_busqueda">
                    <legend class="titulo_legend trigger click">[% "Registro Hijo" | i18n %]</legend>
                        <pre>
                            [% registro_ejemplar.getRegistroMARCOriginal.as_formatted %]
                        </pre>
                    </fieldset>
                [% END %]

            </fieldset>
        </div>

        <div id="tabs-2">

        </div>

    </div>
    </div>
</fieldset>

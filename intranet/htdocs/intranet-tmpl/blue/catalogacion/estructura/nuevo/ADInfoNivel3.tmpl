<hr>
<table cellspacing="0" cellpadding="0"  width="100%" class="tablaResult" width="100%">
    <tr align="center" cellpadding="0"  valign="middle" class="tablaresultadoTitle">

        <td width="1%" align="left" valign="top"><img src="[% themelang %]/images/izquierda.png"></td>

        <td><b>Sig. Top.</b></td>
        <td><b>C&oacute;digo</b></td>
        <td><b>Disponibilidad</b></td>
        <td><b>Origen UI</b></td>
        <td><b>Due&ntilde;o UI</b></td>
        <td><b>Tipo Pr&eacute;stamo</b></td>
        <td><b>Acci&oacute;n</b></td>

        <td width="1%" align="right" valign="top"><img src="[% themelang %]/images/derecha.png"></td>

    </tr>
[% FOREACH nivel3 IN nivel3_array %]
        <tr align="center">

            <td>Id2: [% nivel3.getId2 %]</td>
            <td>[% nivel3.getSignatura_topografica %]</td>
            <td>
                <a href='/cgi-bin/koha/busquedas/detalleItem.pl?id1=[% id1 %]&id2=[% id2 %]&id3=[% id3 %]&barcode=[% nivel3.getBarcode %]&signatura_topografica=[% nivel3.getSignatura_topografica %]' title='Detalle de disponibilidad'>[% nivel3.getBarcode %]</a>
            </td>
            <td>
                [% nivel3.ref_disponibilidad.getNombre %]
            </td>
            <td>
                 [% nivel3.getId_ui_origen %] 
            </td>
            <td>
                [% nivel3.getId_ui_poseedora %]
            </td>
            <td>
                [% IF nivel3.getPara_Sala == 1%]
                    Para Sala
                [% ELSE %]
                    Domiciliario
                [% END %]
            </td>
            <td>
                <input  type="image" value="modificar" src="[% themelang %]/images/editar.png" 
                        onClick="modificarN3('[% nivel3.getId3 %]')" title="Editar Ejemplar">
                   <input TYPE="image" name="delete"  VALUE="delete" src="[% themelang %]/images/borrar.png" onClick="if (window.confirm('&#191;Est&aacute; seguro que desea borrarlo?')){borrarEjemplar('[% id3 %]')} else {return false;}" title="Borrar Ejemplar">
            </td>
            <td></td>

    </tr>
[% END %] <!-- END FOREACH nivel3 -->
</table>


[% IF socio.tieneAutorizado %]
   <fieldset>
        <legend class="titulo_legend">[% "Informaci&oacute;n del usuario Adicional" | i18n %] </legend>
        <ul>
        <li class="sub_item"> <label for="auth_selected">[% 'Adicional' | i18n %]</label>
                            <input  type="checkbox" id="auth_selected" name="auth_selected" 
                                    value="autorizado">
        </li>
        <li class="reference">click para indicar si se presta al usuario adicional</li>
        <li class="sub_item"> <label for="auth_nombre">[% "Nombre y Apellido" | i18n %]</label>
                            <input  type=text id="auth_nombre" name="auth_nombre" 
                                    value="[% socio.getNombre_apellido_autorizado %]" size="25" >
        </li>
        <li class="sub_item">
        <label for="auth_dni">[% "Documento" | i18n %]</label>
                            <input  type="text" id="auth_dni" name="auth_dni" 
                                    value="[% socio.getDni_autorizado %]" size="25" >
        </li>
        <li class="sub_item">
                            <label for="auth_telefono">[% "Tel&eacute;fono" | i18n %]</label>
                            <input  type="text" id="auth_telefono" name="auth_telefono" 
                                    value="[% socio.getTelefono_autorizado %]" size="25" >
        </li>
        </ul>
    </fieldset>
[% END %]

<!--*****************************************Detalle*****************************************************  -->
[% FOREACH loopnivel2 %]

<table cellspacing=0 border="0" cellpadding=0 width="100%" class="tope">
  <tr class="bordetabla"> 
       <th >&nbsp;</th> 
 </tr>
 <tr class="bordetabla"> 
       <th >&nbsp;</th> 
 </tr>
</table>

<div id="content[% DivMARC %]" >
  <center>
    <input type="button" id="volver[% DivMARC %]" value="Volver" style="display: none"
    onClick="Volver('[% DivDetalle %]', '[% DivMARC %]')">
  </center>
  <div id="[% DivMARC %]"> </div>
</div>

<div id="[% DivDetalle %]">

<table cellspacing="2" cellpadding="2" width="100%">
  <tr><td><table class="tablaDetalle" width="100%">
<ul>
[% FOREACH loopnivel1 %]
	[% IF librarian %]
	  [% IF dato %]
	<li>
		[% librarian %]: <b>[% dato %]</b>
	</li>
	  [% END %]
	[% END %]
[% END %]
</ul>

[% FOREACH loopEncabezados %]
	<div align="left">
		<span class="Encabezados">[% encabezado %]</span>
	</div>

	<ul>
	
	[% FOREACH resultado %]
	
		[% IF dato %]
		<li>
			[% librarian %] [% dato %]
		</li>
		[% END %]
	
	[% END %]

	</ul>

[% END %]
  </table></td></tr>
</table>

<table id="tablaDetalleNivel" class="tablaDetalleNivel3" cellspacing=0 border="0" cellpadding=0 width="100%" class="tope">
<thead>
<tr class="bordetabla" align="center">
	<td>&nbsp;</td>
	<td>[% "C&oacute;digo" | i18n %]</td>
	<td>[% "Signatura Topogr&aacute;fica" | i18n %]</td> 
	<td>[% "Biblioteca que lo posee" | i18n %]</td>
	<td>[% "Biblioteca de origen" | i18n %]</td>
	<td>[% "Estado" | i18n %]</td>
	<td>[% "Disponibilidad" | i18n %]</td>
	<td>[% "MARC" | i18n %]</td>
	<td>&nbsp;</td>
</tr>
</thead>
<tbody>
[% FOREACH loopnivel3 %]
	[% FOREACH nivel3 %]
<tr align="center">

	<td>&nbsp;</td>

	<td>[% barcode %]</td>
	<td>[% signatura_topografica %]</td>
	<td>[% holdingbranchNombre %]</td>
	<td>[% homebranchNombre %]</td>
	<td>[% wthdrawnDescrip %]</td>
	<td>[% issuetype %]</td>
	<td>
		<a class="click" onClick="MARCDetail('[% id3 %]', '[% DivDetalle %]', '[% DivMARC %]');">
		<img src="[% themelang %]/images/MARC.png" border="0" alt="Detalle MARC" title="Detalle MARC">
		</a>
	</td>

	<td>&nbsp;</td>

</tr>
	[% END %]
</tbody>
</table>

<table  width="100%">
	<tr>
		<td class="impar" >
		<a href="/cgi-bin/koha/opac-shelves.pl?viewShelfItems=[% biblioitemnumber %]&biblio=[% biblionumber %]">
			<img src="[% themelang %]/images/vertodos.png" border="0" alt="Estantes Virtuales"></a>
	
		<a  href="/cgi-bin/koha/opac-analysis.pl?bibnum=[% biblionumber %]&bibnumitems=[% biblioitemnumber %]" class="inputFontNormal" >
			<img src="[% themelang %]/images/analiticas.png" border="0" alt="[% 'Anal&iacute;ticas' | i18n %] "></a>

		<a  href="/cgi-bin/koha/opac-indiceLibro.pl?biblionumber=[% biblionumber %]&biblioitemnumber=[% biblioitemnumber %]" class="inputFontNormal" >
		<img src="[% themelang %]/images/indice.png" border="0" alt="Indice"></a>
	
		<br>
		[% "Disponibilidad Actual Para Pr&eacute;stamo:" | i18n %] <b>[% cantParaPrestamo %]</b>
		<br>	
		[% "Disponibilidad Actual Para Sala de Lectura:" | i18n %] <b>[% cantParaSala %]</b>
		<br>
		[% "Cantidad Actual de Reservas:" | i18n %] <b>[% "cantResevasActual" | i18n %]</b>
		<br>
			 [% IF !allunavail %]
				[% IF allshared %]
				<p align="right"><b>[% "Ejemplares compartidos" | i18n %]</b></p>
				[% ELSE %]
				[% IF allcopy %]
				<p align="right"><b>[% "Solicitar copia" | i18n %]</b></p>
				[% ELSE %]
					[% IF cantParaPrestamo %]

						[% IF CirculationEnabled %]  <!-- si es 0 -->

						<p align="right">
						<a class="click" 
						onClick="reservar([% id1 %],[% id2 %])">
						<b>Reservar</b>
						</a></p>

						[% ELSE %]
			<!-- Si el usuario no esta logueado, se le da la siguiente informacion-->
						<p align="right"><b>[% "Disponible para Reservar" | i18n %]</b></p>
						[% END %]
					
					[% ELSE %]
						

						<p align="right"><b>[% "Disponible s&oacute;lo para consulta en Sala de Lectura" | i18n %]</b></p>

					[% END %]
				  [% END %]
				[% END %]
			[% END %]
		</td>
        </tr>
</table>
	 [% END %]

</div>

[% END %]
<!-- end loopnivel2 -->

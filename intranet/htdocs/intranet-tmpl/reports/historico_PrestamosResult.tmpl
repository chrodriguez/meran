
<div class="resultados_consulta">[% "La cantidad de pr&eacute;stamos es:" | i18n %] [% cantidad %]</div> 

<div class="contenedor_paginador">[% paginador %]</div>

<table id="tablaResult" class="tablaResult tabla_datos" cellspacing="0" cellpadding="0" border="0" width="100%">
<tr height="24" class="tablaresultadoTitle">

	<td class="click" onclick="ordenar('surname')" title="[% 'Ordenar por apellido' | i18n %]">
        [% "Apellido" | i18n %]
    </td>
	<td class="click" onclick="ordenar('firstname')" title="[% 'Ordenar por nombre' | i18n %]">
        [% "Nombre" | i18n %]
    </td>
	<td class="click" onclick="ordenar('DNI')" title="[% 'Ordenar por DNI' | i18n %]">
        [% "DNI" | i18n %]
    </td>
	<td class="click" onclick="ordenar('CatUsuario')" title="[% 'Ordenar por Categoria' | i18n %]">
        [% "Categor&iacute;a" | i18n %]
    </td>
	<td class="click" onclick="ordenar('tipoPrestamo')" title="[% 'Ordenar por tipo de prestamo' | i18n %]">
        [% "Tipo Pr&eacute;stamo" | i18n %]
    </td>
	<td class="click" onclick="ordenar('barcode')" title="[% 'Ordenar por codigo de barra' | i18n %]">
        [% "C&oacute;digo de Barras" | i18n %]
    </td>
	<td class="click" onclick="ordenar('fechaPrestamo')" title="[% 'Ordenar por fecha de prestamo' | i18n %]">
        [% "Fecha Pr&eacute;stamo" | i18n %]
    </td>
	<td class="click" onclick="ordenar('fechaDevolucion')" title="[% 'Ordenar por fecha de devolucion' | i18n %]">
        [% "Fecha Devoluci&oacute;n" | i18n %]
    </td>
	<td class="click" onclick="ordenar('tipoItem')" title="[% 'Ordenar por tipo de documento' | i18n %]">
        [% "T&iacute;po" | i18n %]
    </td>

</tr>
	
[% FOREACH historico_pretamo IN RESULTSLOOP %]
    <tr align="center">

            <td align="left">[% historico_pretamo.socio.persona.getApellido %]</td>
            <td>[% historico_pretamo.socio.persona.getNombre %]</td>
            <td>[% historico_pretamo.socio.persona.getNro_documento %]</td>
            <td>[% historico_pretamo.socio.getCod_categoria %]</td>
            <td>[% historico_pretamo.tipo.getDescripcion %]</td>
            <td>[% historico_pretamo.nivel3.getBarcode %]</td>
            <td>[% historico_pretamo.getFecha_prestamo_formateada %]</td>
            <td>[% historico_pretamo.getFecha_devolucion_formateada %]</td>
            <td>[% historico_pretamo.nivel3.nivel2.cat_ref_tipo_nivel3.getNombre %]</td>
    
    </tr>
[% END %]
</table>

<div align="center">
		<br><br>
	<form name="prestamos" id="prestamos" method="post">
		<!-- Para imprimir correctamentes los datos -->
		<input type="hidden" name="tiposItems" value="[% tipoItem %]">
		<input type="hidden" name="catUsuarios" value="[% catUsuarios %]">
		<input type="hidden" name="tipoPrestamos" value="[% tipoPrestamo %]">
		
		<input type="hidden" name="ini" value="[% ini %]">
		<input type="hidden" name="fin" value="[% fin %]">
		<input type="hidden" name="orden" value="[% orden %]">
        <input type="hidden" name="token" value="[% token %]">
		<!-- Para ver que se hace, imprimir o generar un pdf -->
		<input TYPE="hidden" VALUE="pdf" NAME="type" > 


<div class="buttonContainerHorizontal">

        [% PERL %]
                                print C4::AR::Filtros::to_Icon(
                                                                boton   => "icon_imprimir",
                                                                onClick => "imprimir();",
                                                                title   => "[% "Imprimir Resultados" | i18n %]",
                                        ) ;
        [% END %]

        [% PERL %]
                            print C4::AR::Filtros::to_Icon(
                                                            boton   => "icon_pdf",
                                                            onClick => "generarPDF();",
                                                            title   => "[% "Generar PDF" | i18n %]",
                                    ) ;
        [% END %]

</div>

	</form>
</div>
<br>

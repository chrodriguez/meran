
<!-- ************************************Window Permisos Usuario***************************************** -->
		
	<table width="100%"  border="0" cellpadding="0" cellspacing="0">
		<tr align="center"><td class="titulos_interior" >[% socio.persona.getApeYNom %]</td></tr>
		<tr><td class="lineagruesa"  align="left" colspan="3"></td></tr>
	</table>

	<table border=0 width="100%" cellpadding="0" cellspacing="0">

	[% FOREACH loop %]
		<tr>
			<td >&nbsp;</td>
			<td>
				<input name="chkpermisos" class="inputFontNormal" type=checkbox value=[% bit %] [% checked %]/>
			</td>
			<td class="inputFontNormal">
				[% flag %]
			</td>
			<td class="inputFontNormal">
				[% flagdesc %]
			</td>
			<td >&nbsp;</td>
		</tr>

	[% END %]

	</table>

    <div>
        [% PERL %]
                            print C4::AR::Filtros::to_Button(   text    => "[% 'Guardar Permisos' | i18n %]",
                                                                boton   => "boton_buscar",
                                                                onClick => "guardarPermisos();",
                                                                title   => "[% 'Guardar Permisos' | i18n %]",
                                    ) ;
        [% END %]
    </div>

<!-- *******************************Fin**Window Permisos Usuario***************************************** -->

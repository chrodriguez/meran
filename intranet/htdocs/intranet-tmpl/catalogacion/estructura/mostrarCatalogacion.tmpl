[% IF nivel != 'Niveles' %]
    <br>

    [% IF cant != 0 %]
        <table id="tablaCamposMod" class="tabla_datos" width="100%" cellspacing="0" cellpadding="0" border="0">
	        <tr>
		        
		        <td colspan="7"> 
			        <b>Campos Modificados del nivel [% nivel %] </b>
		        </td>
		        
	        </tr>
            
	        <tr class="titulo_tabla_resultado">
		        <td width="21%">MARC</td>
		        <td width="20%"> Vista en Intranet</td>
		        <td>Orden</td>
		        <td >Visible</td>
		        <td >Editar</td>
		        <td >Deshabilitar</td>
                <td >Debug</td>
	        </tr>
        [% FOREACH catalogacion IN catalogaciones %]
	        <tr align="center">
		        <td>
			        Campo: <b>[% catalogacion.getCampo %]</b>, Subcampo: <b>[% catalogacion.getSubcampo %]</b>
		        </td>
		        <td>
                    [% catalogacion.getIntranet_habilitado %]
			        [% catalogacion.getLiblibrarian %]
		        </td>
		        <td>
                <div class="botonCentradoTabla">
<!--                 Si no soy el prmiero muestro el boton para subir el orden -->
                [% IF ! catalogacion.primeroDelTipo(itemType) %]  
<!--			        <input  src="[% themelang %]/images/1uparrow.png" type="image"  value="Subir en el Orden" title="Subir Orden" 
                    onClick="modificarOrden('[% catalogacion.getId %]', 'SUBIR_ORDEN')"> -->
                        [% PERL %]
                            print C4::AR::Filtros::to_Icon(    
                                                                boton   => "icon_subir",
                                                                onClick => "modificarOrden('[% catalogacion.getId %]', 'SUBIR_ORDEN');",
                                                                title   => "[% 'Subir en el Orden' | i18n %]",
                                    ) ;
                        [% END %]
                [% END %]
<!--                 Si no soy el ultimo muestro el boton para bajar el orden -->
                [% IF ! catalogacion.ultimoDelTipo(itemType) %]  
			        <!--<input  src="[% themelang %]/images/1downarrow.png" type="image"  value="Bajar en el Orden" title="Bajar Orden" 
                    onClick="modificarOrden('[% catalogacion.getId %]', 'BAJAR_ORDEN')">-->
                        [% PERL %]
                            print C4::AR::Filtros::to_Icon(    
                                                                boton   => "icon_bajar",
                                                                onClick => "modificarOrden('[% catalogacion.getId %]', 'BAJAR_ORDEN');",
                                                                title   => "[% 'Bajar en el Orden' | i18n %]",
                                    ) ;
                        [% END %]
                [% END %]
                </div>
		        </td>
		        <td>
				[% IF catalogacion.getObligatorio != 1 %]
			        <input type="checkbox" id="[% id %]" value="[% catalogacion.getVisible %]" onChange="visibilidad('[% catalogacion.getId %]')" 
			        value="[% catalogacion.getVisible %]"   [% IF catalogacion.getVisible %] 
                                                                checked
			                                                [% END %] 
                    >
				[% END %]
		        </td>
		        <td>
                <div class="botonCentradoTabla">
                [% IF catalogacion.soyFijo != 1 %]
                        [% PERL %]
                            print C4::AR::Filtros::to_Icon(    
                                                                boton   => "icon_edicion",
                                                                onClick => "mostrarModificarCampo('[% catalogacion.getId %]');",
                                                                title   => "[% 'Modificar Campo' | i18n %]",
                                    ) ;
                        [% END %]
                [% END %]
                </div>
		        </td>
		        <td>
                <div class="botonCentradoTabla">
                [% IF catalogacion.soyFijo != 1 %]
                        [% PERL %]
                            print C4::AR::Filtros::to_Icon(    
                                                                boton   => "icon_borrar",
                                                                onClick => "eliminarCampo('[% catalogacion.getId %]');",
                                                                title   => "[% 'Eliminar Campo' | i18n %]",
                                    ) ;
                        [% END %]
                [% END %]
                </div>
		        </td>
		        <td>    
                        <!-- AGREGADO SOLO PARA DEBUGUEAR -->
                        <br>&nbsp;  [% IF catalogacion.soyFijo == 1 %]
                                            Campo Fijo
                                    [% END %]
						-  [% IF catalogacion.getObligatorio == 1 %]
                                            Obligatorio
                                    [% END %]
                </td>
	        </tr>
        [% END %]
            
        </table>
    
    [% END %]
    
    <div class="botonCentrado">
            [% PERL %]
                     print C4::AR::Filtros::to_Button(      text    => "[% 'Agregar Campo' | i18n %]",
                                                            boton   => "boton_agregar",
                                                            onClick => "mostrarAgregarCampo();",
                                                            title   => "[% 'Agregar Campo' | i18n %]",
                                                            width   => "135",
                             ) ;
            [% END %]
    </div>
       
[% END %] <!-- END IF niveles -->
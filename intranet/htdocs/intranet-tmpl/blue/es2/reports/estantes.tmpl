function consultar(){
	var shelf=$("#shelf").val();
	$.ajax({
		type: "POST",
		url: "/cgi-bin/koha/reports/estantesResult.pl",
		data: "shelf="+shelf,
		beforeSend: Init,
		complete: function(ajax){
				$("#result").html(ajax.responseText);
				zebra("tablaResult");
			}
	})
}

$(document).ready( function() {
	consultar();
});
</script>


[% INCLUDE "intranet-top.inc" %]

<link rel="stylesheet" type="text/css" href="[% themelang %]/includes/epoch_styles.css ">

<script src="/includes/jquery/datetimepicker/ui.datepicker.js"></script>
<link href="/includes/jquery/datetimepicker/flora.datepicker.css" type="text/css" rel="stylesheet">
<script src="/includes/jquery/validate/jquery.validate.js"></script>

<script language="javascript" type="text/javascript">

$(document).ready(function() {

    crearDatePicker('fechaIni');

    crearDatePicker('fechaFin');

});

</script>

<script type="text/javascript">


var objAH;
function ordenar(orden){
	objAH.sort(orden);
}

function changePage(ini){
	objAH.changePage(ini);
}

function updateInfo(responseText){
	$("#result").html(responseText);
	zebra("tablaResult");
}

function consultar(){
	objAH=new AjaxHelper(updateInfo);
    objAH.debug= true;
	objAH.url="/cgi-bin/koha/reports/logueoBusquedaResult.pl";
	objAH.fechaIni=$("#fechaIni").val();
	objAH.fechaFin=$("#fechaFin").val();
	objAH.catUsuarios=$("#categoria_socio_id").val();
	objAH.funcion= "changePage";
	objAH.sendToServer();
}
</script>

<table width="100%"  border="0" cellpadding="0" cellspacing="0">
	<tr>
		<td class="pagetitle" >[% "Logueo de B&uacute;squedas desde OPAC" | i18n %]</td>
	</tr>
	<tr>
		<td class="lineagruesa"  align="left" colspan="3"></td>
	</tr>
</table>

<br>
<center>

<ul>
	<li>
	  <b>[% "Seleccione una fecha de inicio: " | i18n %]</b>
	</li>
	<li>
		<input id="fechaIni" class="inputFontNormal" name="fechaIni" value="[% fechaIni %]"></li>
	<li><b>[% "Seleccione una fecha de Fin: " | i18n %]</b></li>	
	<li>
	  	<input id="fechaFin" class="inputFontNormal" name="fechaFin" value="[% fechaFin %]"></li>
	<li><b>[% "Categoria de Usuario:" | i18n %] </b></li>
	<li>[% selectCatUsuarios  %]</li>
	<li>
		<input type="image" name="consulta" src="[% themelang %]/images/generarListado.png" onClick="consultar('');">
	</li>
</ul>

<div id="result"></div>



<p class="inputFontNormal" align="center">
        [% "La cantidad de ejemplares es:" | i18n %] [% cantidad %]
</p>

<table id="tablaResult" class="tablaResult" border="0" cellspacing="0" cellpadding="0" width="100%">
    <tr class="tablaresultadoTitle">

            <th onClick="ordenar('barcode');">[% "Nro. de Inventario" | i18n %]</th>
            <th onClick="ordenar('completo');">[% "Autor" | i18n %]</th> 
            <th onClick="ordenar('titulo');">[% "T&iacute;tulo" | i18n %]</th>
            <th onClick="ordenar('number');">[% "Edic." | i18n %]</th>
            <th onClick="ordenar('publisher');">[% "Editor" | i18n %]</th>
            <th onClick="ordenar('anio_publicacion');">[% "A&ntilde;o" | i18n %]</th>
            <th onClick="ordenar('signatura_topografica');">[% "Signatura Topogr&aacute;fica" | i18n %]</th>

    </tr>
    [% FOREACH results %]
        <tr>
            <td width="1%">&nbsp;</td>
            <td  align=left class="inputFontNormal">
                   [% barcode %]
            </td>
            <!-- falta que se ve el campo del autor -->
            <td align="center" class="inputFontNormal">
                    [% autor %]
            </td>
            <td align="center" class="inputFontNormal">
                    [% titulo %]
                    [% IF unititle %]:&nbsp;
                        [% unititle %]
                    [% END %]
            </td>
            <td align="center" class="inputFontNormal">
                    [% number %]
            </td>
            <td align="center" class="inputFontNormal">
                    [% publisher %]
            </td>
            <td align="center" class="inputFontNormal">
                    [% anio_publicacion %]
            </td>
            <td align="center" class="inputFontNormal">
                    [% signatura_topografica %]
            </td>
            <td width="1%">&nbsp;</td>
        </tr>
    
    [% END %]
</table>

<br><br>
<center>
    <a href="/cgi-bin/koha/reports/plantillas/generatesxc.pl?name=[% name %]" class="inputFontNormal" onMouseOut="swap('planillas','[% themelang %]/images/plantilla.png')" onMouseOver="swap('planillas','[% themelang %]/images/plantillabis.png')" >
        <input  name="planillas" id="planillas" alt=[% "Generar planilla" | i18n %] type="image" value=[% "Generar planilla" | i18n %]  src="[% themelang %]/images/plantilla.png" border="0" title=[% "Generar planilla" | i18n %] >
    </a>
</center>



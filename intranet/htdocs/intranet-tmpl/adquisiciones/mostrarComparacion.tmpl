<fieldset>
             
                            [% FOREACH det IN detalle_pedido %]
                                    <li>DATOS DEL EJEMPLAR</li>
                                    [% IF det.getAdqRecomendacionDetalle %]
                                          <li>Titulo:[%  det.getAdqRecomendacionDetalle.getTitulo  %] - Autor:[%  det.getAdqRecomendacionDetalle.getAutor  %]</li>
                                    [% ELSE %] 
                                          <li><strong>Titulo:[%  det.geTitulo  %] - Autor:[%  det.getAutor  %]</strong></li>
                                    [% END %]
                                   
                                    [% FOREACH pres IN presupuestos %]
                                            
                                            <li><strong>[% pres.precio_unitario %]</strong></li>
                                            <li><strong>[% pres.cant %]</strong></li>
                                            <li><strong>[% pres.total %]</strong></li>
                                    [% END %]                    
                            [% END %]

         
          
</fieldset>
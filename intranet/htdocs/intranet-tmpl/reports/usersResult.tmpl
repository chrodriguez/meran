[% paginador %]
</center>


<table id="tablaResult" class="tablaResult" border="0" cellspacing="0" cellpadding="0"  width="100%">
	<tr class="tablaresultadoTitle">
		<td width="1%" align="left" valign="top"><img src="[% themelang %]/images/izquierda.png"></td> 

		<td class="tablaresultadoTitle click" onclick="ordenar('surname')" Title="[% 'Ordenar por Apellido' | i18n %]">[% "Apellido" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('firstname')" Title="[% 'Ordenar por Nombre' | i18n %]">[% "Nombre" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('streetaddress')" Title="[% 'Ordenar por Direcci&oacute;n' | i18n %]">[% "Direcci&oacute;n" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('cardnumber')" Title="[% 'Ordenar por Socio' | i18n %]">[% "Socio" | i18n %]</td>
		<td class="tablaresultadoTitle">[% "Ciudad" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('phone')" Title="[% 'Ordenar por Tel&eacute;fono' | i18n %]">[% "Telefono" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('emailaddress')" Title="[% 'Ordenar por e-mail' | i18n %]">[% "E-mail" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('dateenrolled')" Title="[% 'Ordenar por Ingreso' | i18n %]">[% "Ingreso" | i18n %]</td>
		<td class="tablaresultadoTitle click" onclick="ordenar('categoria')" Title="[% 'Ordenar por Categor&iacute;a' | i18n %]">[% "Categoria" | i18n %]</td>

		<td width="1%" align="right" valign="top"><img src="[% themelang %]/images/derecha.png"></td>
	</tr>

[% FOREACH resultsloop %]
	<tr class="inputFontNormal">
		<td>&nbsp;</td>
		<td> [% surname %] </td>
		<td> [% firstname %] </td>
  		<td> [% streetaddress %] </td>
  		<td> [% cardnumber %] </td>
  		<td> [% city %] </td>
  		<td> [% phone %]</td>
  		<td> [% IF ok %] 
                        [% emailaddress %]
                     [% ELSE %]
                        <a class="inputFontNormal" href="mailto:[% emailaddress %]">[% emailaddress %] </a>
                    [% END %]</td>
  		<td> [% dateenrolled %] </td>
  		<td> [% categoria %]</td>
  	</tr>

[% END %]

	<tr valign=bottom>
	        <td class="lineafinaltabla"  align="left" colspan="9"></td>
        </tr>

</table>


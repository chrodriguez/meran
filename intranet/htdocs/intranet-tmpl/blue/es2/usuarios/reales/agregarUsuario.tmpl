<script language="javascript" type="text/javascript">

function swap(id,src){
document.getElementById(id).src=src;
}

function validarInput(campo,error){
	if(campo.value==""){
		alert(error);
		campo.focus();
		var ancla="blanco"+campo.id;
		crearAncla(campo.id,ancla);
		location.href="#"+ancla;
		return false;
	}
	return true;
}

function chequearCampos(){
	var i=1;
	var error="Campo obligatorio vacio, ingrese un valor";
	var ok=true;
	var idError;
	while((i<6) && (ok)){
		var campo= document.getElementById(i);
		ok=validarInput(campo,error);
		if(ok){
			i++;
		}
	}
	if(ok){
		var city=document.getElementById('city');
		ok=validarInput(city,error);
	}
	return ok;
}
	
function chequearDocumento(){
	var docInput=document.getElementById('4');
	var doc = docInput.value.toString();
	for (var i=0; i<doc.length; i++) {
		var caracter = doc.charAt(i);
		if (caracter<"0" || caracter>"9"){
			alert("Error al ingresar el numero de documento, no es un numero tiene que ser solo digitos");
			docInput.focus();
			var ancla="blanco"+'4num';
			crearAncla('4',ancla);
			location.href="#"+ancla;
			return false;
		}
	}
	return true;
}
	
function mandarForm(){
	if(chequearCampos() && chequearDocumento()){
		document.form1.submit();
	}
}

function CrearAutocomplete(Id){
	$("#"+Id).search();
	// q= valor de campoHelp
	$("#"+Id).autocomplete('/cgi-bin/koha/autocompletables/searchcity.pl',{
 		formatItem: function(row){
			return row[1];
		},
		minChars:1,
        	matchSubset:1,
        	matchContains:1,
		maxItemsToShow:10,
        	cacheLength:10,
        	selectOnly:1,
	});//end autocomplete
	$("#"+Id).result(function(event, data, formatted) {
		$("#"+Id).val(data[1]);
	});
}


[% INCLUDE "intranet-top.inc" %]

<link rel="stylesheet" type="text/css" href="[% themelang %]/includes/epoch_styles.css ">
<script type="text/javascript">
[% INCLUDE "epoch_classes.js" %]

/*You can also place this code in a separate file and link to it like epoch_classes.js*/
        var dp_ini,dp_fin;      
	window.onload = function () {
		        dp_ini  = new Epoch('epoch_popup','popup',document.getElementById('ini'));
			dp_fin  = new Epoch('epoch_popup','popup',document.getElementById('fin'));
				};

function hacerSubmit(){
        document.unidadInformacion.submit();
        }
</script>

<table width="100%" class="pagetitle" border="0" cellpadding="0" cellspacing="0" ><tr><td>[% "Disponibilidad" | i18n %]</td></tr>
         <tr><td class="lineagruesa"  align="left" colspan="3"></td></tr>
</table>

<center>
<br>
<br>
<form name="unidadInformacion" action=/cgi-bin/koha/reports/avail_year.pl method=post >
<table border=0 cellspacing=0 cellpadding=0 width="100%" align="left">
<tr>
<td class="inputFontNormal">[% "Seleccione una unidad de informaci&oacute;n:" | i18n %]</td>
<td class="inputFontNormal">[% unidades %]</td>
<td>&nbsp;</td>
</tr>
<tr>
<td class="inputFontNormal">[% "Seleccione una fecha de inicio:" | i18n %]</td>
<td class="inputFontNormal"> 
		<input id="ini" name="ini" value="[% ini %]">
	     	<a href="javascript:dp_ini.toggle();">
				<img src="[% themelang %]/images/cal.gif" width="16" height="16" border="0" alt="Selecciona una fecha">
		</a>
</td>
<td>&nbsp;</td>
</tr>
<tr>
<td class="inputFontNormal">[% "Seleccione una fecha de Fin:" | i18n %]</td>
<td class="inputFontNormal"> 
		<input  id="fin" name="fin" value="[% fin %]">
		<a href="javascript:dp_fin.toggle();">
			<img src="[% themelang %]/images/cal.gif" width="16" height="16" border="0" alt="Selecciona una fecha">
		</a>
</td>
<td class="inputFontNormal"> 
	<input type="image" name="consulta" onClick="unidadInformacion.submit()" src="[% themelang %]/images/generarListado.png">
</td>	
   </tr>
<tr><td colspan="3">&nbsp;</td></tr>
</table>
<br>
<br>
<table border=0 cellspacing=0 cellpadding=0 width="100%"  align="left">
<tr class="tablaresultadoTitle">
<td width="1%" align="right"><img src="[% themelang %]/images/izquierda.png"></td>
<td>[% "Mes" | i18n %]</td>
<td>[% "A&ntilde;o" | i18n %]</td>
<td>[% "Disponibilidad" | i18n %]</td>
<td>[% "Cantidad" | i18n %]</td>
<td width="1%"><img src="[% themelang %]/images/derecha.png"></td>
</tr>
[% FOREACH resultsloop %]
  <tr align='center'>
  <td height=25 class="[% clase %]">&nbsp;</td>
  <td class="[% clase %]"> [% mes %] </td>
  <td class="[% clase %]"> [% year %] </td>
  <td class="[% clase %]"> [% avail %] </td>
  <td class="[% clase %]"> [% cantidad %] </td>
  <td height=25 class="[% clase %]">&nbsp;</td>
  </tr>
[% END %]
</table>
</form>

<br><br>
<br>


<script type="text/javascript">
      $(document).ready(function() {
        makeToggle('datos_tabla_div','trigger',null,false);
      });
</script>
<fieldset class="resultados_busqueda">
    <legend class="titulo_legend trigger click">[% "Resultados de la b&uacute;squeda (cat&aacute;logo)" | i18n %]:</legend>
        <div class="datos_tabla_div" id="datos_tabla_div">
            <div class="resultados_consulta">[% "T&eacute;rmino de b&uacute;squeda" | i18n %]: [% buscoPor %]</div>
            <div class="resultados_consulta">[% "Total de registros" | i18n %]: [% cantidad %]  en ([% timeSeg %] segundos)</div>
            <div class="contenedor_paginador">[% paginador %]</div>
                <table id="tablaResult" class="datos_tabla" border=0 cellspacing=0 cellpadding=0 width="100%">
                    <tr class="titulo_tabla_resultado">
                        <th>[% "Mes" | i18n %]</th>
                        <th>[% "Prestamos" | i18n %]</th>
                        <th>[% "Renovaciones" | i18n %]</th>
                        <th>[% "Devoluciones" | i18n %]</th>
                        <th>[% "Domiciliarios" | i18n %]</th>
                        <th>[% "Fotocopia" | i18n %]</th> 
                        <th>[% "Sala de Lectura" | i18n %]</th>
                        <th>[% "Prestamos Especiales" | i18n %]</th>
                    </tr>
                
                [% FOREACH prestamo IN estadisticas_anuales %]
                    <tr align='center'>
                        
                        <td > 
                            [% prestamo.getFecha_prestamo_formateada %]
                        </td>
                        <td > 
                            [% prestamo.agregacion_temp %]
                        </td>
                        <td > 
                            [% IF renovaciones %] 
                                            [% renovaciones %]
                            [% ELSE %]
                                            -- 
                                        [% END %]
                        </td>
                        <td > 
                            [% prestamo.getDevoluciones %]
                        </td>
                        <td >
                            [% IF DO %] 
                                            [% DO %]
                            [% ELSE %]
                                            -- 
                                        [% END %]
                        </td> 
                        <td >
                            [% IF FO %]
                                            [% FO %]
                            [% ELSE %]
                                            -- 
                                        [% END %]
                        </td> 
                        <td >
                            [% IF SA %]
                                            [% SA %]
                            [% ELSE %]
                                            -- 
                                        [% END %]
                        </td>
                        <td >
                            [% IF ES %]
                                            [% ES %]
                            [% ELSE %]
                                            -- 
                                        [% END %]
                        </td>
                        
                    </tr>
                [% END %]
                    <tr valign=bottom>
                        <td class="lineafinaltabla"  align="left" colspan="9"></td>
                    </tr>
                </table>
        </div>
</fieldset>

		
[% IF prestamos_cant %]

[% USE HTML %]

<div class="titulo_arriba_tabla">[% "EJEMPLARES PRESTADOS" | i18n %]</div>
	<table class="prestamosResult tabla_datos" cellspacing=0  cellpadding=0 border=0  width="100%" > 
	
		<tr valign=middle class="tablaresultadoTitle">
	
			<th>
				<a class="click" id="checkAllPrestamos">
				<img src="[% themelang %]/images/check.png" border="0" 
					title='[% "Seleccionar todos" | i18n %]'>
				</a>
			</th>
	
			<th>[% "T&iacute;tulo" | i18n %]</th>
			<th>[% "Autor" | i18n %]</th>
			<th>[% "C. Barra" | i18n %]</th>
			<th>[% "Sig. Top." | i18n %]</th>
			<th>[% "Ed." | i18n %]</th>
			<th>[% "Vol." | i18n %]</th>
			<th>[% "Pr&eacute;stamo" | i18n %]</th>
			<th>[% "Vencimiento" | i18n %]</th>
			<th>[% "Tipo" | i18n %]</th>
	
		</tr>
	[% FOREACH prestamo IN PRESTAMOS %]
		<tralign="center">
	
			<td>
		[% IF circularDesdeDetalleUsuario %]
			<input type="checkbox" name="chkboxPrestamos" value="[% prestamo.getId_prestamo %]" id="[% prestamo.getId_prestamo %]">
		[% ELSE %]
			[% 'NO SE PERMITE CIRCULAR' | i18n %]
		[% END %] <!--END IF permiso_para_circular -->
			</td>
			<td width="14%" >
			[% PERL %]
				print C4::AR::Filtros::link_to(	text => 	"[% prestamo.nivel3.nivel1.getTitulo %]",
												url => 		"/cgi-bin/koha/catalogacion/estructura/detalle.pl", 
												params =>	["id1=[% prestamo.nivel3.nivel1.getId1 %]"],
											) ;
			[% END %]
			</td>
			<td>
			[% IF prestamo.nivel3.nivel1.getAutor %]
				[% prestamo.nivel3.nivel1.cat_autor.getCompleto %]
			[% ELSE %]
				&nbsp;
			[% END %]
			</td>
	
			<td>
			[% PERL %]
				print C4::AR::Filtros::link_to(	text => 	"[% prestamo.nivel3.getBarcode %]",
												url => 		"/cgi-bin/koha/busquedas/detalleItem.pl", 
												params =>	["id1=[% prestamo.nivel3.nivel1.getId1 %]", "id2=[% prestamo.nivel3.nivel2.getId2 %]", 
																"id3=[% prestamo.getId3 %]","barcode=[% prestamo.nivel3.getBarcode %]",
															"signatura_topografica=[% prestamo.nivel3.getSignatura_topografica %]"],
												title =>	"[% 'Detalle de disponibilidad' | i18n %]"
											) ;
			[% END %]
			</td>
	
			<td>
				[% IF prestamo.nivel3.getSignatura_topografica %] 
					[% PERL %]
						print C4::AR::Filtros::link_to(	text => 	"[% prestamo.nivel3.getSignatura_topografica %]",
														url => 		"/cgi-bin/koha/detalle.pl", 
														params =>	["id1=[% prestamo.nivel3.nivel1.getId1 %]"],
											) ;
					[% END %]
				[% END %]
			</td>
	
			<td>  
			[% IF prestamo.nivel3.nivel2.getEdicion %] [% prestamo.nivel3.nivel2.getEdicion %] [% ELSE %] - [% END %]
			</td>
	
			<td> 
			[% IF prestamo.nivel3.nivel2.getVolumen %] t [% prestamo.nivel3.nivel2.getVolumen %] 
				[% IF prestamo.nivel3.nivel2.getVolumenDesc %] ([% prestamo.nivel3.nivel2.getVolumenDesc %])  [% END %] 
			[% END %]
			</td>
			<td>
			[% IF prestamo.getFecha_prestamo %] 
				[% prestamo.getFecha_prestamo_formateada %]
			[% ELSE %]
				- 
			[% END %]
			</td>
	
			<td>
				[% IF prestamo.getFecha_vencimiento %]
			<font color="[% IF prestamo.estaVencido %]red[% END %]">[% prestamo.getFecha_vencimiento_formateada %]</font>
				[% ELSE %]
					- 
				[% END %]
			</td>
	
			<td>
				[% IF prestamo.tipo.id_tipo_prestamo %]
						[% prestamo.tipo.getDescripcion %]
				[% END %]
			</td>
			
		</tr>
	[% END %] <!-- FOREACH prestamos -->
	
	</table>
	
		<p align="center">
	[% IF circularDesdeDetalleUsuario %]	
		[% IF prestamos_cant %]	
			<input type="image" BORDER=0 
			src="[% themelang %]/images/item-return.png" 
			title="Devolver" 
			onClick="realizarAccion('DEVOLUCION','chkboxPrestamos',generaDivDevolucion);">
		[% END %]	
		
		[% IF socio.esRegular %]
			[% UNLESS socio.estaSancionado %]
				<input type="image" 
				src="[% themelang %]/images/renew.png" title="Renovar" 
				onClick="realizarAccion('RENOVACION','chkboxPrestamos',generaDivRenovacion);">
			[% END %]
		[% END %]
	[% ELSE %]
			[% 'NO SE PERMITE CIRCULAR' | i18n %]
	[% END %] <!--END IF permiso_para_circular -->
		</p>

[% END %]

		
[% IF prestamos_cant %]
	[% USE HTML %]
	<p align=center><b>[% "EJEMPLARES PRESTADOS" | i18n %]</b></p>
	<table class="prestamosResult" cellspacing=0  cellpadding=0 border=0  width="100%" > 
	
		<tr valign=middle class="tablaresultadoTitle">
			<th width="1%" valign="top" align="right" valing="top">
			<img src="[% themelang %]/images/izquierda.png">
			</th>
	
			<th>
				<a class="click" id="checkAllPrestamos">
				<img src="[% themelang %]/images/check.png" border="0" 
					title='[% "Seleccionar todos" | i18n %]'>
				</a>
			</th>
	
			<th>[% "T&iacute;tulo" | i18n %]</th>
			<th>[% "Autor" | i18n %]</th>
			<th>[% "C. Barra" | i18n %]</th>
			<th>[% "Sig. Top." | i18n %]</th>
			<th>[% "Ed." | i18n %]</th>
			<th>[% "Vol." | i18n %]</th>
			<th>[% "Pr&eacute;stamo" | i18n %]</th>
			<th>[% "Vencimiento" | i18n %]</th>
			<th>[% "Tipo" | i18n %]</th>
	
			<th width="1%" valign="top">
			<img src="[% themelang %]/images/derecha.png">
			</th>
		</tr>
	[% FOREACH prestamo IN PRESTAMOS %]
		<tr class="inputFontNormal" align="center">
			<td height=25 >&nbsp;</td>
			<td>
				<input type="checkbox" name="chkboxPrestamos" value="[% prestamo.getId_prestamo %]" id="[% prestamo.nivel3.getBarcode %]">
			</td>
			<td width="14%" >
				<a href="/cgi-bin/koha/busquedas/detalle.pl?id1=[% prestamo.nivel3.nivel1.getId1 %]">
					[% prestamo.nivel3.nivel1.getTitulo %]
					<!--[% IF unititle %]
						:&nbsp;[% HTML.escape(unititle) %]
					[% END %]-->
				</a>
			</td>
			<td>
				[% IF prestamo.nivel3.nivel1.getAutor %]
					[% prestamo.nivel3.nivel1.cat_autor.getCompleto %]
				[% ELSE %]
					&nbsp;
				[% END %]
			</td>
	
			<td>
				<a href='/cgi-bin/koha/busquedas/detalleItem.pl?id1=[% prestamo.nivel3.nivel1.getId1 %]&id2=[% prestamo.nivel3.nivel2.getId2 %]&id3=[% prestamo.getId3 %]&barcode=[% prestamo.nivel3.getBarcode %]&signatura_topografica=[% prestamo.nivel3.getSignatura_topografica %]' title='Detalle de disponibilidad'> 
				[% prestamo.nivel3.getBarcode %]</a>
			</td>
	
			<td>
				[% IF prestamo.nivel3.getSignatura_topografica %] 
					<a class="inputFontNormal" 
						href="/cgi-bin/koha/detalle.pl?id1="[% prestamo.nivel3.nivel1.getId1 %]">
						[% prestamo.nivel3.getSignatura_topografica %] 
					</a>
				[% END %]
			</td>
	
			<td>  
			[% IF prestamo.nivel3.nivel2.getEdicion %] [% prestamo.nivel3.nivel2.getEdicion %] [% ELSE %] - [% END %]
			</td>
	
			<td> 
			[% IF prestamo.nivel3.nivel2.getVolumen %] t [% prestamo.nivel3.nivel2.getVolumen %] 
				[% IF prestamo.nivel3.nivel2.getVolumenDesc %] ([% prestamo.nivel3.nivel2.getVolumenDesc %])  [% END %] 
			[% END %]
			</td>
			<td>
			[% IF prestamo.getFecha_prestamo %] 
				[% prestamo.getFecha_prestamo_formateada %]
			[% ELSE %]
				- 
			[% END %]
			</td>
	
			<td>
				[% IF prestamo.getFecha_vencimiento %]
			<font color="[% IF prestamo.estaVencido %]red[% END %]">[% prestamo.getFecha_vencimiento_formateada %]</font>
				[% ELSE %]
					- 
				[% END %]
			</td>
	
			<td>
				[% IF prestamo.tipo.id_tipo_prestamo %]
						[% prestamo.tipo.getDescripcion %]
				[% END %]
			</td>
			<td>&nbsp;</td>	
		</tr>
	[% END %] <!-- FOREACH prestamos -->
	
	</table>
	
	<p align="center">
	
	[% IF prestamos_cant %]	
		<input type="image" BORDER=0 
		src="[% themelang %]/images/item-return.png" 
		title="Devolver" 
		onClick="realizarAccion('DEVOLUCION','chkboxPrestamos',generaDivDevRen);">
	[% END %]	
	
	[% IF socio.esRegular %]
		[% UNLESS sanctions %]
			<input type="image" 
			src="[% themelang %]/images/renew.png" title="Renovar" 
			onClick="realizarAccion('RENOVACION','chkboxPrestamos',generaDivDevRen);">
		[% END %]
	[% END %]
	</p>
[% END %]

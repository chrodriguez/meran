<fieldset class="resultados_busqueda">
    <legend class="titulo_legend">[% "Hist&oacute;rico de Disponibilidad" | i18n %]:</legend>
        <div class="contenedor_paginador">[% paginador %]</div>
        <div class="datos_tabla_div_user">
            <table id="tablaResult" class="datos_tabla" border=0 cellspacing=0 cellpadding=0 width=100%>
                <thead>
                <tr class="titulo_tabla_resultado">
                    <th>[% "Disponibilidad" | i18n %]</th>
                    <th>[% "Estado" | i18n %]</th>
                    <th>[% "Fecha" | i18n %]</th>

                </tr>
                </thead>
                <tbody>
                    [% FOREACH historico IN historico_disponibilidad %]
                        <tr>
                            <td>
                                [% historico.getTipo_prestamo %] 
                            </td>
                            <td>
                                [% historico.getDetalle %] 
                            </td>
                            <td>
                                [% historico.getFecha %] 
                            </td>
                        </tr>
                    [% END %]
                </tbody>
            </table>
            <div class="resultados_consulta">[% "La cantidad en el historial de disponibilidad es" | i18n %]: [% cant_historico %]</div>
            <div class="contenedor_paginador">[% paginador %]</div>
        </div>
</fieldset>
[% IF nuevoPermiso %]
    <h5 style="font-size: 20px; color:red;"> [% "ATENCION: DANDO DE ALTA UN NUEVO PERMISO YA QUE EL USUARIO NO CUENTA CON PRIVILEGIOS PARA TAL COMBINACION " | i18n %] </h5>
[% END %]
<hr>
<div style="height:50px;" id="reportes">
    <h2> Reportes </h2>
    <span class="permission_grant" >
        <label for="permission_5"> TODOS </label>
            <input type="checkbox" id="reportes_todos" name="reportes_todos" onChange="adviceGrant(this,'reportes','high');"
                [% IF permisos.reportes.TODOS %] "checked" [% END %] />
        <label for="permission_2"> Baja </label>
            <input type="checkbox" id="reportes_baja" name="reportes_baja" onChange="adviceGrant(this,'reportes','high');"
                [% IF permisos.reportes.BAJA %] "checked" [% END %]/> /
        <label for="permission_3"> Modificaci&oacute;n </label>
            <input type="checkbox" id="reportes_modif" name="reportes_modif" onChange="adviceGrant(this,'reportes','high');"
                [% IF permisos.reportes.MODIFICACION %] "checked" [% END %] /> /
        <label for="permission_1"> Alta </label>
            <input type="checkbox" id="reportes_alta" name="reportes_alta" onChange="adviceGrant(this,'reportes','medium');"
                [% IF permisos.reportes.ALTA %] "checked" [% END %] /> /
        <label for="permission_4"> Consulta </label>
            <input type="checkbox" id="reportes_consulta" name="reportes_consulta" onChange="adviceGrant(this,'reportes','low');"
                [% IF permisos.reportes.CONSULTA %] "checked" [% END %]/>
    </span>
<br><br></div>

<hr>
<div style="height:50px;" id="preferencias">
    <h2> Preferencias </h2>
    <span class="permission_grant" >
        <label for="permission_5"> TODOS </label>
            <input type="checkbox" id="preferencias_todos" name="preferencias_todos" onChange="adviceGrant(this,'preferencias','high');"
                [% IF permisos.preferencias.TODOS %] "checked" [% END %] />
        <label for="permission_2"> Baja </label>
            <input type="checkbox" id="preferencias_baja" name="preferencias_baja" onChange="adviceGrant(this,'preferencias','high');"
                [% IF permisos.preferencias.BAJA %] "checked" [% END %]/> /
        <label for="permission_3"> Modificaci&oacute;n </label>
            <input type="checkbox" id="preferencias_modif" name="preferencias_modif" onChange="adviceGrant(this,'preferencias','high');"
                [% IF permisos.preferencias.MODIFICACION %] "checked" [% END %] /> /
        <label for="permission_1"> Alta </label>
            <input type="checkbox" id="preferencias_alta" name="preferencias_alta" onChange="adviceGrant(this,'preferencias','medium');"
                [% IF permisos.preferencias.ALTA %] "checked" [% END %] /> /
        <label for="permission_4"> Consulta </label>
            <input type="checkbox" id="preferencias_consulta" name="preferencias_consulta" onChange="adviceGrant(this,'preferencias','low');"
                [% IF permisos.preferencias.CONSULTA %] "checked" [% END %]/>
    </span>
    <br><br>
</div>

<div class="buttonContainerHorizontal">
    <ul class="horizontal-list" >
        [% IF nuevoPermiso != 1%]
                        [% PERL %]
                                            print C4::AR::Filtros::to_Button(   text    => "[% 'Guardar Permisos' | i18n %]",
                                                                                boton   => "boton_buscar",
                                                                                onClick => "actualizarPermisos();",
                                                                                title   => "[% 'Guardar Permisos' | i18n %]",
                                                    );
                        [% END %]
        [% ELSE %]
                [% PERL %]
                                    print C4::AR::Filtros::to_Button(   text    => "[% 'Guardar Permisos' | i18n %]",
                                                                        boton   => "boton_buscar",
                                                                        onClick => "nuevoPermiso();",
                                                                        title   => "[% 'Guardar Permisos' | i18n %]",
                                            ) ;
                [% END %]
        [% END %]
  </ul>
  <br>
</div>

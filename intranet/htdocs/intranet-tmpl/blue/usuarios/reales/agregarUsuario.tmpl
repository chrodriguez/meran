<script language="javascript" type="text/javascript">

function validateNro_socio(){
    nro_socio = $('#nro_socio').val();
    if ( (($.trim(nro_socio)).length == 0 ) ) {
        $('#nro_socio').val('Auto-generar'); 
        $('#nro_socio').attr("readonly","true"); 
        $('#nro_socio').attr("title","Solo sera numerico");
    } 
}
    

function toggleNro_socio(){
    if ($('#nro_socio').attr("readonly") == true){ 
        $('#nro_socio').val(''); 
        $('#nro_socio').removeAttr("readonly"); 
        $('#nro_socio').removeAttr("title"); 
    } 
}
$(document).ready(function() {
   [% IF addBorrower %]
      validateForm(agregarUsuario);
   [% ELSE %]
      validateForm(guardarModificacioUsuario);
   [% END %]

    validateNro_socio();
    $('#nro_socio').click(function(){toggleNro_socio()});
    $('#nro_socio').blur(function(){validateNro_socio()});
    $("#userDataForm").validate();
    CrearAutocompleteCiudades('ciudad', 'id_ciudad','/cgi-bin/koha/autocompletables/searchcity.pl');
    CrearAutocompleteCiudades('alt_ciudad', 'id_alt_ciudad','/cgi-bin/koha/autocompletables/searchcity.pl');
    $('#nacimiento').datepicker({ dateFormat: 'dd/mm/yy' });

	$('#categoria_socio_id').addClass('required');
    
});

</script>


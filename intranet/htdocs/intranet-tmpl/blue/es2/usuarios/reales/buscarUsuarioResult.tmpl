[% USE HTML %]

<table border=0 cellspacing=0 cellpadding=2 width=90%>
	<tr align="center">
		<b>[% "La cantidad de usuarios es :" | i18n %] [% cantidad %]</b>
	</tr>

	<tr align="center">
		<b>[% "Ha buscado por :" | i18n %] [% HTML.escape(socio) %]</b>
	</tr>
</table>
<br>
<center>
[% paginador %]
</center>
<table id="tablaResult" class="tablaResult" border=0 cellspacing=0 cellpadding=0 width=100%>
	<thead>
  	<tr>
    		<th class="tablaresultadoTitle" width="1%" align="left" valign="top">
			<img src="[% themelang %]/images/izquierda.png">
		</th>

		<th class="tablaresultadoTitleClick" onclick="ordenar('nro_socio')" 
		Title="Ordenar por tarjeta de Id." width="12%">[% "Tarjeta de Id." | i18n %]</th>
    		<th class="tablaresultadoTitleClick" onclick="ordenar('apellido')" 
		Title="Ordenar por apellido" align="left" width="30%">[% "Apellido y Nombres" | i18n %]</th>
    		<th class="tablaresultadoTitleClick" onclick="ordenar('nro_documento')" 
		Title="Ordenar por DNI" width="15%">[% "Documento" | i18n %]</th>
    		<th class="tablaresultadoTitleClick" onclick="ordenar('legajo')" 
		Title="Ordenar por legajo" align="left">[% "Legajo" | i18n %]</th>
    		<th class="tablaresultadoTitleClick" onclick="ordenar('cod_categoria')" 
		Title="Ordenar por categoria">[% "Cat." | i18n %]</th>
    		<th class="tablaresultadoTitle">[% "Prest." | i18n %]</th>
    		<th class="tablaresultadoTitle">[% "Ven." | i18n %]</th>
    		<th class="tablaresultadoTitle">[% "Estado" | i18n %]</th>


   		<th class="tablaresultadoTitle" width="1%" valign="top" align="right">
			<img src="[% themelang %]/images/derecha.png">
		</th>	
  	</tr>
   	</thead>
	<tbody>

[% FOREACH resultsloop %]	
  	<tr  align="center" class="inputFontNormal">
     		<td height=25>&nbsp;</td>

      		<td align='left'>
			[% socio.getNro_socio %]
		</td>
     		<td width=250 align="left">
			<a class="inputFontNormal" 
			href="/cgi-bin/koha/usuarios/reales/datosUsuario.pl?id_socio=[% socio.getId_socio %]">
				[% socio.persona.getApellido %], [% socio.persona.getNombre %]
			</a>
		</td>
      		<td align="left">
			[% socio.persona.getVersion_documento %]&nbsp;[% socio.persona.getNro_documento %]
		</td>
      		<td align="left">
			[% socio.getLegajo %]
		</td>
      		<td align=center>
			[% socio.categoria.getDescription %]
		</td>
      		<td align=center>
			[% issue %]
		</td>
      		<td align=center>
			[% od %]
		</td>
      		<td align="center">
			<span class="[% clase %]"><b>[% regular %]</b></span>
		</td>

 		<td height=25>&nbsp;</td>
    	</tr>

[% END %]

  	<tr>
 		<td class="lineafinaltabla"  align="left" colspan="14"></td>
	</tr>
	</tbody>

</table>


// FIXME FALTA VALIDAD EN EL CLIENTE
    
function CrearAutocomplete(Id){
    $("#"+Id).search();
    // q= valor de campoHelp
    $("#"+Id).autocomplete('/cgi-bin/koha/autocompletables/searchcity.pl',{
        formatItem: function(row){
            return row[1];
        },
        minChars:1,
            matchSubset:1,
            matchContains:1,
        maxItemsToShow:10,
            cacheLength:10,
            selectOnly:1,
    });//end autocomplete
    $("#"+Id).result(function(event, data, formatted) {
        $("#"+Id).val(data[1]);
    });
}


$(document).ready(function() {
        
    CrearAutocomplete('city');
    CrearAutocomplete('dstreetcity');

    $('#nacimiento').datepicker({ dateFormat: 'dd/mm/yy' });
});

</script>


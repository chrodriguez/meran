[% FOREACH nivel2 IN nivel2_array %]    
    <p class="nivel2Selected" onClick="mostrarInfoAltaNivel3( '[% nivel2.getId1 %]',  '[% nivel2.getId2 %]'); toggleClass(this);">
    <b>Grupo: [% nivel2.getId2 %] - [% nivel2.cat_ref_tipo_nivel3.getNombre %]</b>
    <br/>
    <div>
        [% PERL %]
                print C4::AR::Filtros::to_Button(   text    => "[% 'Modificar Nivel 2' | i18n %]",
                                                    boton   => "boton_buscar",
                                                    onClick => "modificarN2('[% nivel2.getId2 %]');",
                                                    title   => "[% 'Modificar nivel 2' | i18n %]",
                        ) ;
        [% END %]
    </div>
    <div>
        [% PERL %]
                print C4::AR::Filtros::to_Button(   text    => "[% 'Agregar Nivel 3' | i18n %]",
                                                    boton   => "boton_buscar",
                                                    onClick => "agregarN3();",
                                                    title   => "[% 'Agregar nivel 3' | i18n %]",
                        ) ;
        [% END %]
    </div>
    <div>
        [% PERL %]
                print C4::AR::Filtros::to_Button(   text    => "[% 'Borrar Nivel 1' | i18n %]",
                                                    boton   => "boton_buscar",
                                                    onClick => "borrarN2('[% nivel2.getId2 %]');",
                                                    title   => "[% 'Borrar nivel 2' | i18n %]",
                        ) ;
        [% END %]
    </div>


    Nivel bibliogr&aacute;fico: [% nivel2.ref_nivel_bibliografico.getDescription %]
    <br/>
    Soporte: [% nivel2.ref_soporte.getDescription %]
    <br/>
    Lugar: [% nivel2.pais_publicacion.getNombre_largo %]
    <br/>
    Idioma: [% nivel2.ref_idioma.getDescription %]
    <br/>
    Fecha: [% nivel2.getAnio_publicacion %]
    <br/>
    Cantidad Ejemplares: [% cant_ejemplares %]
    </p>
[% END %]
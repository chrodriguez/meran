


<table id="tablaSeudonimos" class="tablesorter" border=0 cellspacing=0 cellpadding=0 width="100%" align="center">
	<thead>
 	<tr class="tablaresultadoTitle">

		<th width="1%" align="left" valign="top"><img src="[% themelang %]/images/izquierda.png"></th>
	
		<th>Seud&oacute;nimo</th>
		<th>Eliminar</th>
	
		<th width="1%" align="rigth" valign="top"><img src="[% themelang %]/images/derecha.png"></th>
	</tr>

	</thead>

	<tbody>
[% FOREACH seudonimo IN RESULTSLOOP %]
	<tr align='center' class="tablaresultadoTitle">

		<td height=25 class="[% clase %]">&nbsp;</td>

		<td class="[% clase %]" class="inputFontNormal"> 
            [% IF seudonimo.seudonimo.getApellido %]
			    [% seudonimo.seudonimo.getApellido %], [% seudonimo.seudonimo.getNombre %] <!-- PARA AUTOR -->
            [% END %]
            [% seudonimo.seudonimo.getNombre %] <!-- PARA TEMA -->
            [% seudonimo.seudonimo.getEditorial %] <!-- PARA EDITORIAL -->
		</td>
		<td class="[% clase %]" align="center">
			<a class="click" 
			onClick="deleteSeudonimo('[% seudonimo.getIdAutorSeudonimo %]'+'[% seudonimo.getIdTemaSeudonimo %]'+'[% seudonimo.getIdEditorialSeudonimo %]');"><img src="[% themelang %]/images/borrar.png" Title="Eliminar" ALT='Eliminar' hspace='0' vspace='0' border='0'></a>
		</td>

		<td height=25 class="[% clase %]">&nbsp;</td>

	</tr>
[% END %]
	<tr valign=bottom>
	        <td class="lineafinaltabla"  align="left" colspan="9"></td>
        </tr>
	</tbody>
</table>


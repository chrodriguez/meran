<table id="tablaResultado" class="tablesorter" cellspacing="0" cellpadding="0" border="0" width="100%">
	<thead>

	<tr class="tablaresultadoTitle">
		<th width="1%" align="left" valign="top">
		<img src="[% themelang %]/images/izquierda.png">
		</th>

		<th width="13%" class="header">[% "Variable" | i18n %]</th>
		<th width="25%" class="header">[% "Valor" | i18n %]</th>
		<th width="39%" class="header">[% "Explicaci&oacute;n" | i18n %]</th>
		<th width="3%">Editar [% "Editar" | i18n %]</th>
		<th width="1%" align="right" valign="top">
		<img src="[% themelang %]/images/derecha.png">
		</th>
	</tr>

	</thead>
	<tbody>


[% FOREACH preferencia IN preferencias %]
	<tr class="inputFontNormal">
		<td height="25">&nbsp;</td>
		<td >[% preferencia.getVariable %]</td>
		<td >[% preferencia.getValue %]</td>
		<td >[% preferencia.getExplanation %]</td>
		<td align="center">
		<input type="image" src="[% themelang %]/images/editar.png"
		alt="Editar" border="0" 
		onClick="modificarVariable('[% preferencia.getVariable %]','[% preferencia.getType %]')">
		</td>
		<td height="25">&nbsp;</td>
	</tr>
[% END %]

	</tbody>
</table>

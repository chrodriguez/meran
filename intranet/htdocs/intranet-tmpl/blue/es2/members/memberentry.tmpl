<script>
	function validarInput(campo,error){
		if(campo.value==""){
			alert(error);
			campo.focus();
			return false;
		}
		return true;
	}

	function chequearCampos(){
		var i=1;
		var error="Campo obligatorio vacio, ingrese un valor";
		var ok=true;
		for(i;i<6;i++){
			var campo= document.getElementById(i);
			if(ok){
				ok=validarInput(campo,error);
			}
		}
		if(ok){
			var city=document.getElementById('dcity');
			ok=validarInput(city,error);
		}
		return ok;
	}
	
	function chequearDocumento(){
		var docInput=document.getElementById('4');
		var doc = docInput.value.toString();
		for (var i=0; i<doc.length; i++) {
			var caracter = doc.charAt(i);
			if (caracter<"0" || caracter>"9"){
				alert("Error al ingresar el numero de documento, no es un numero tiene que ser solo digitos");
				docInput.focus();
				return false;
			}
		}
		return true;
	}
	
	function mandarForm(){
		if(chequearCampos() && chequearDocumento()){
			document.form1.submit();
		}
	}

</script>


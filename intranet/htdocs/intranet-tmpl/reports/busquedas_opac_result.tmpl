[% USE HTML %]

<script type="text/javascript">
      $(document).ready(function() {
        makeToggle('datos_tabla_div','trigger',null,false);
      });
</script>

<fieldset class="resultados_busqueda">
    <legend class="titulo_legend trigger click">[% "Resultados de la b&uacute;squeda (cat&aacute;logo)" | i18n %]:</legend>
        <div class="datos_tabla_div" id="datos_tabla_div">
            <div class="resultados_consulta">[% "Total de registros" | i18n %]: [% cantidad %]  en ([% timeSeg %] segundos)</div>
            <div class="contenedor_paginador">[% paginador %]</div>
                <table id="tablaResult" class="datos_tabla" cellspacing="0" cellpadding="0" border="0" width="100%">
                    <tr class="titulo_tabla_resultado">
                    
                        <th>
                            [% "Apellido y Nombres" | i18n %] 
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(1,'hide_nom_ape')">&nbsp;</span>
                        </th>
                        <th>
                            [% "N&uacute;mero de socio" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(2,'hide_nro_socio')">&nbsp;</span>
                        </th>
                        <th >
                            [% "Categor&iacute;a" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(3,'hide_categoria')">&nbsp;</span>
                        </th>
                        <th >
                            [% "Fecha" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(4,'hide_fecha')">&nbsp;</span>
                        </th>
                        <th >
                            [% "Campo" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(5,'hide_campo')">&nbsp;</span>
                        </th>
                        <th >
                            [% "Valor" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(6,'hide_valor')">&nbsp;</span>
                        </th>
                        <th >
                            [% "Tipo" | i18n %]
                            <span class="boton_eliminar_X click" style="float:right;" title="[% 'Eliminar Columna' | i18n %]" onclick="hideTableColumn(7,'hide_tipo')">&nbsp;</span>
                        </th>
                
                    </tr>
                
                    </tr>
                [% FOREACH busqueda IN logs_busqueda %]
                    <tr align='center'>
                        <td>
                            [% PERL %]
                                print C4::AR::Filtros::link_to( text =>     "[% busqueda.busqueda.socio.persona.getApeYNom %]",
                                                                url=>"[% url_prefix %]/usuarios/reales/datosUsuario.pl", 
                                                                params =>   ["nro_socio=[% busqueda.busqueda.socio.getNro_socio %]"],
                                                                title =>    "[% 'Mostrar Detalle del Usuario' | i18n %]"
                                                            ) ;
                            [% END %]
                        </td>
                        <td>[% IF busqueda.busqueda.socio.getNro_socio %]
                                [% busqueda.busqueda.socio.getNro_socio %]
                            [% ELSE %]
                                [% 'Usuario no logueado' | i18n %]
                            [% END %]
                        </td>
                        <td>[% busqueda.busqueda.socio.getCod_categoria %]</td>
                        <td>[% busqueda.busqueda.fecha %]</td>
                        <td>[% busqueda.campo %]</td>
                        <td>[% HTML.escape(busqueda.getValor) %]</td>
                        <td>[% busqueda.getTipo %]</td>
                
                    </tr>
                [% END %]
                    <tr valign=bottom>
                            <td class="lineafinaltabla"  align="left" colspan="9"></td>
                    </tr>
                
                </table>
        </div>
        <div class="buttonContainerHorizontal">
          [% PERL %]
                  print C4::AR::Filtros::to_Button( 
                                                      text    =>  "[% "Exportar XLS" | i18n %]",
                                                      title    =>  "[% 'Exportar XLS' | i18n %]",
                                                      boton   =>  "boton_guardar",
                                                      onClick =>  "tableToXLS('[% filename %]');",
                                                  );
          [% END %]
      </div>
        <form id="exportPDF" action="[% url_prefix %]/reports/busquedas_opac.pl?token=[% token %]">
                <input type="hidden" name="export" value="pdf" />
                <input type="hidden" name="total" value="[% param_total %]" />
                <input type="hidden" name="registrados" value="[% param_registrados %]" />
                <input type="hidden" name="tipo_socio" value="[% param_tipo_socio %]" />
                <input type="hidden" name="f_inicio" value="[% param_f_inicio %]" />
                <input type="hidden" name="f_fin" value="[% param_f_fin %]" />
                
                <input type="hidden" name="hide_nom_ape" id="hide_nom_ape" value=0 />
                <input type="hidden" name="hide_nro_socio" id="hide_nro_socio" value=0 />
                <input type="hidden" name="hide_categoria" id="hide_categoria" value=0 />
                <input type="hidden" name="hide_fecha" id="hide_fecha" value=0 />
                <input type="hidden" name="hide_campo" id="hide_campo" value=0 />
                <input type="hidden" name="hide_valor" id="hide_valor" value=0 />
                <input type="hidden" name="hide_tipo" id="hide_tipo" value=0 />
        </form>


        <div class="buttonContainerHorizontal">
                [% PERL %]
                    print C4::AR::Filtros::to_Button( 
                                                        text    =>  "[% 'Exportar a PDF' | i18n %]",
                                                        boton   =>  "boton_buscar",
                                                        onClick =>  "submitForm('exportPDF');",
                            ) ;
                [% END %]
        </div>
</fieldset>
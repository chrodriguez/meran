[% INCLUDE "intranet-top.inc" %]

<script>

function consultar(){
        if ( !(isNaN($("#year").val())) ){
            objAH=new AjaxHelper(updateInfo);
            objAH.showOverlay       = true;
            objAH.url= "/cgi-bin/koha/reports/estadistica_AnualResult.pl";
            objAH.year= $("#year").val();
            //se envia la consulta
            objAH.sendToServer();
        }
}

function updateInfo(responseText){
	$("#result").html(responseText);
	zebra("tablaResult");
}

</script>

<div class="titulos" >[% "Actividades anuales" | i18n %]</div>

<fieldset>
    <legend class="titulo_legend">[% "Buscar signatura topogr&aacute;fica: &nbsp;" | i18n %]</legend>
        <ul>
            <li class="sub_item">
                <label for="">[% "Seleccione un a&ntilde;o:  " | i18n %] [% year %]</label>
            </li>
        </ul>
</fieldset>

<div id="result"></div>

[% INCLUDE "intranet-bottom.inc" %]

<fieldset>
              <table id="tablaResult2" class="datos_tabla" border=0 cellspacing=0 cellpadding=0 width=100%>
              <thead>
                        <tr class="titulo_tabla_resultado">
                            <th id="renglon">
                                [% "Presupuesto - Renglon" | i18n %]
                            </th>
                            [% FOREACH prov IN proveedores %] 
                                <th id="renglon">
                                    [% "Proveedor" | i18n %]: [% prov%]
                                </th>
                            [% END %]
                        </tr>
               
              </thead>
              <tbody>    
                     [% FOREACH col IN matriz %]  
                          <tr>
                                [% FOREACH celda IN col %]  
                                        <td> [% celda.precio_unitario %]</td>      
                                [% END %]
                           </tr>   
                     [% END %]
              </tbody>   
          </table>
</fieldset>
		
[% IF circulateEnable %]
	[% USE HTML %]
	<p align=center><b>EJEMPLARES PRESTADOS</b></p>
	<table class="prestamosResult" cellspacing=0  cellpadding=0 border=0  width="100%" > 
	
		<tr valign=middle class="tablaresultadoTitle">
			<th width="1%" valign="top" align="right" valing="top">
			<img src="[% interface %]/[% themelang %]/images/izquierda.png">
			</th>
	
			<th>
				<a class="click" id="checkAllPrestamos">
				<img src="[% themelang %]/images/check.png" border="0" 
					title="Seleccionar todos">
				</a>
			</th>
	
			<th>T&iacute;tulo</th>
			<th>Autor</th>
			<th>C. Barra</th>
			<th>Sig. Top.</th>
			<th>Ed.</th>
			<th>Vol.</th>
			<th>Pr&eacute;stamo</th>
			<th>Vencimiento</th>
			<th>Tipo</th>
	
			<th width="1%" valign="top">
			<img src="[% interface %]/[% themelang %]/images/derecha.png">
			</th>
		</tr>
	[% FOREACH prestamos %]
		<tr class="inputFontNormal" align="center">
			<td height=25 >&nbsp;</td>
			<td>
				<input type="checkbox" name="chkboxPrestamos" value="[% id3 %]" id="[% barcode %]">
			</td>
			<td width="14%" >
				<a href="/cgi-bin/koha/busquedas/detalle.pl?id1=[% id1 %]">
					[% IF unititle %]
						:&nbsp;[% HTML.escape(unititle) %]
					[% END %]
				</a>
			</td>
			<td>
				[% IF autor %]
					[% autor %]
				[% ELSE %]
					&nbsp;
				[% END %]
			</td>
	
			<td>
				<a href='/cgi-bin/koha/busquedas/detalleItem.pl?id1=[% id1 %]&id2=[% id2 %]&id3=[% id3 %]&barcode=[% barcode %]&signatura_topografica=[% signatura_topografica %]' title='Detalle de disponibilidad'>[% barcode %]</a>
			</td>
	
			<td>
				[% IF signatura_topografica %] 
					<a class="inputFontNormal" 
						href="/cgi-bin/koha/detalle.pl?id1="[% id1 %]">
						[% signatura_topografica %] 
					</a>
				[% END %]
			</td>
	
			<td>  
				[% IF edicion %]
					[% edicion %]
				[% END %]
			</td>
	
			<td>  
				[% IF volume %] 
					[% volume %]
	
					[% IF volumeddesc %]
						: [% volumeddesc %]
					[% END %]
	
				[% END %]
			</td>
			<td>
				[% IF date_due %]
					[% date_due %]
				[% END %]
			</td>
	
			<td>
				[% IF date_fin %] 
					<font color="[% color %]">
						[% date_fin %]
					</font>
				[% END %]
			</td>
	
			<td>
				[% IF issuetype %]
					<font color="[% color %]">
						[% issuetype %]
					</font>
				[% END %]
			</td>
			<td>&nbsp;</td>
	[% END %] <!-- FOREACH prestamos -->
		</tr>
	</table>
	
	<p align="center">
	
	[% IF circulateEnable %]	
		<input type="image" BORDER=0 
		src="[% interface %]/[% theme %]/images/item-return.png" 
		title="Devolver" 
		onClick="realizarAccion('DEVOLUCION','chkboxPrestamos',generaDivDevRen);">
	[% END %]	
	
	[% IF regular %]
		[% UNLESS sanctions %]
			<input type="image" 
			src="<!-- TMPL_VAR name='interface' -->/<!-- TMPL_VAR name='theme' -->/images/renew.png" title="Renovar" 
			onClick="realizarAccion('RENOVACION','chkboxPrestamos',generaDivDevRen);">
		[% END %]
	[% END %]
	</p>

[% END %] <!-- END CIRCULATE ENABLE -->


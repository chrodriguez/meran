  `usercode` varchar(10) default NULL,
  `itemnumber` int(11) default NULL,
  `itemtype` varchar(4) default NULL,
  `borrowernumber` int(11) default NULL,
  KEY `timeidx` (`datetime`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


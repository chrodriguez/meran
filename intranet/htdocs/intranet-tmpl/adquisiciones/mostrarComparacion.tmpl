<fieldset>
                   
                              [% FOREACH pres. %]     
                                <fieldset>   
                                     <li>Renglon: [% detalle.nro_renglon %]</li>
                                     <li>Autor: [% pres.detalle.autor %]</li>
                                     <li>Titulo: [% pres.detalle.titulo %]</li>
                      
                                </fieldset> 
                         
                                     [% FOREACH p IN pres.presupuesto %]
                                         
                                                <li>[% p.proveedor %] </li>
                                           
                                           <li>[% IF p.precio_unitario == 0 %]
                                                             <span>No cotiza</span>
                                               [% ELSE %]
                                                             <span>[% p.cant%] -</span> [% p.precio_unitario %] - [% p.total %]</span>
                                               [% END %]       
                                                                            </li> 
                                    
                                    [% END %]
                                          
                                [% END %]
                                    
                             
                           
                       
          
</fieldset>





                  

<div class="vcard">

<div width="100%"  border="0" cellpadding="0" cellspacing="0" align="center">
	<p class="pagetitle" ><span class="fn">[% socio.persona.getApeYNom %]</span></p>
</div>


<table  cellspacing=0  cellpadding=0 border=0 align=left width="100%" >
	<tr VALIGN=middle class="tablaresultadoTitle">
		<td width="1%" align="left" valing="top">
			<img src="[% themelang %]/images/izquierda.png">
		</td>
		<td colspan=3>[% "REGISTRO DEL USUARIO" | i18n %]</td>
		<td width="1%" valing="top" align="right">
			<img src="[% themelang %]/images/derecha.png">
		</td>
	</tr>
</table>
<br><br>

<div class="divDatosUsuario">
        
        [% "Apellido:" | i18n %]
                <b> [% socio.persona.getApellido %] </b><br>

        [% "Nombre:" | i18n %]
                <b> [% socio.persona.getNombre %] </b><br>

        [% "Documento:" | i18n %]
                <b> [% socio.persona.getVersion_documento %]&nbsp; [% socio.persona.getNro_documento %]</b><br>

        [% "Fecha de nacimiento:" | i18n %]
                <b> [% socio.persona.getNacimiento %]</b><br>
        [% "Sexo:" | i18n %]
                <b> [% socio.persona.getSexo %]</b><br>
        [% IF socio.persona.getTelefono %]
                <div class="tel">
                        <span class="type">[% "Tel&eacute;fono:" | i18n %]</span>
                                <b>[% socio.persona.getTelefono %]</b><br>
                </div>	
        [% END %]

        [% IF socio.persona.getAlt_telefono %]
        <div class="tel">
                <span class="type">[% "Tel&eacute;fono Alt:" | i18n %]</span>
                            <b>[% socio.persona.getAlt_telefono %]</b><br>
        </div>	
        [% END %]
        
        [% IF socio.persona.getEmail %]
                E-mail: <b><a href="mailto:[% emailaddress %]">
                        <span class="email"> [% socio.persona.getEmail %]</span></a></b><br>
        [% END %]
        <br>
            
        [% "Legajo :" | i18n %]
                        <b> [%  socio.persona.getNro_documento %]</b><br>

        [% "Ciudad :" | i18n %]
                        <b> [%  socio.persona.ciudad_ref.getNombre %]</b><br>
        [% "Calle :" | i18n %]
                        <b> [%  socio.persona.getCalle %]</b><br>
                    

</div>
            
</div>  <!--end vcard-->

<div class="divDatosUsuario" style="margin-top: 3%;margin-bottom: 5%">
    <ul>
            <li>
                    <a class="click" onClick="modificarDatosDeUsuario()">
                    <img 	alt="Modificar Caracter&iacute;stica" 
                            src="[% themelang %]/images/modificarcaracteristicas.png" border="0"">
                    </a>
            </li>
    </ul>
</div>
<br>

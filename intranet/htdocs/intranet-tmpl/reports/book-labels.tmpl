[% USE HTML %]

<script language="javascript">
function hacerSubmit(){
	document.cards.op.value="search";
	document.cards.submit();
}

function ordenar(orden){
	
	window.location.href="book-labels.pl?ini=[% ini %]&orden="+orden+"&op=search&barcode1=[% barcode1  %]&barcode2=[% barcode2 %]&ui=[% ui %]&bulk1=[% bulk1 %]&bulk2=[% bulk2 %]&bulkbegin=[% bulkbegin %]";
}

function generarEtiquetas(){
	document.cards.orden.value= [% orden  %];
	document.cards.barcode1.value= [% barcode1 %];
	document.cards.barcode2.value=[% barcode2 %];
	document.cards.bulk1.value=[% bulk1 %];
	document.cards.bulk2.value=[% bulk2 %];
	document.cards.bulkbegin.value=[% bulkbegin %];
	document.cards.ui.value=[% ui %];

	document.cards.op.value='pdf';
	document.cards.submit();
}

</script>


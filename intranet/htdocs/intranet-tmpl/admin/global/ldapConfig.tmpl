[% INCLUDE "intranet-top.inc" %]
    
<fieldset width="100%">
    <legend class="titulo_legend"> [% "Configuraci&oacute;n del servidor LDAP" | i18n %] </legend>
    <div>
        <h2>Usar un servidor LDAP</h2>
        <p>Este método proporciona autenticaci&oacute;n contra un servidor LDAP externo. Si el nombre de usuario y contraseña facilitados son validos, MERAN crea una nueva entrada para el usuario en su base de datos.</p>
    </div>
        
        
<br>
<!--form-->
<form action="/cgi-bin/koha/admin/global/ldap_config.pl" method="post">
<input type="hidden" name="adding" value="1"/>
<h4>Ajustes de servidor LDAP </h4> 
<ul>
    <li class="sub_item">
    <!-- aca usamos una pref_preferencia_sistema que ya existe -->
        <label>[% "URL del host" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Direccion del servidor LDAP' | i18n %]",);
            [% END %]
        </label> 
        <input name="host_url" size="30" value="[% preferencias.ldapserver %]" type="text">
    </li>
    <li class="sub_item">
        <label>[% "Versi&oacute;n" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'La versi&oacute;n del protocolo LDAP que su servidor est&aacute; utilizando.' | i18n %]",);
            [% END %]
        </label>
        <select id="menuversion" class="select menuversion" name="version"> 
           <option value="2" selected="selected">2</option> 
           <option value="3">3</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Codificaci&oacute;n LDAP" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Especifique la codificaci&oacute;n usada por el servidor LDAP. Muy probablemente utf-8, MS AD v2 utiliza codificaci&oacute;n de plataforma por defecto como cp1252, cp1250, etc.' | i18n %]",);
            [% END %]
        </label> 
        <input id="ldapencoding" name="ldapencoding" value="utf-8" type="text"> 
    </li>
    <br>
    <h4>Fijar ajustes </h4> 
    <li class="sub_item">
        <label>[% "Ocultar contrase&ntilde;as" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Seleccione *s&iacute;* para evitar que las contraseñas se almacenen en la base de datos de MERAN.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menupreventpassindb" class="select menupreventpassindb" name="preventpassindb"> 
           <option value="0">No</option> 
           <option value="1" selected="selected">S&iacute;</option> 
        </select> 
    </li>    
    <li class="sub_item">
        <label>[% "Nombre distinguido" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Si quiere usar *bind-user* para buscar usuarios, especif&iacute;quelo aqu&iacute;. Algo como cn=ldapuser,ou=public,o=org' | i18n %]",);
            [% END %]
        </label> 
        <input name="bind_dn" id="bind_dn" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Contraseña para bind-user.' | i18n %]",);
            [% END %]
        </label> 
        <input name="bind_pw" id="bind_pw" size="30" value="" type="password"> 
    </li>
    <br>
    <h4>Ajustes de búsqueda de usuario </h4> 
    <li class="sub_item">
        <label>[% "Tipo de usuario" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Seleccione c&oacute;mo se almacenar&aacute;n los usuarios en LDAP. Este ajuste tambi&eacute;n especifica c&oacute;mo funcionar&aacute;n la caducidad del acceso, los accesos libres y la creaci&oacute;n de usuarios.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuuser_type" class="select menuuser_type" name="user_type"> 
           <option value="edir">Novell Edirectory</option> 
           <option value="rfc2307">posixAccount (rfc2307)</option> 
           <option value="rfc2307bis">posixAccount (rfc2307bis)</option> 
           <option value="samba">sambaSamAccount (v.3.0.7)</option> 
           <option value="ad">MS ActiveDirectory</option> 
           <option value="default" selected="selected">Por defecto</option> 
        </select>  
    </li>
    <li class="sub_item">
        <label>[% "Contextos" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Lista de contextos donde estan localizados los usuarios. Separar contextos diferentes con *;*. Por ejemplo: ou=usuarios,o=org; ou=otros,o=org' | i18n %]",);
            [% END %]
        </label> 
        <input name="contexts" id="contexts" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Buscar subcontextos" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Ponga el valor &lt;&gt; 0 si quiere buscar usuarios desde subcontextos.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menusearch_sub" class="select menusearch_sub" name="search_sub"> 
           <option selected="selected" value="0">No</option> 
           <option value="1">S&iacute;</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Alias de referencia" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Determina c&oacute;mo se manejan los alias durante la busqueda. Seleccione uno de los siguientes valores: *No* (LDAP_DEREF_NEVER) o *Si* (LDAP_DEREF_ALWAYS)' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuopt_deref" class="select menuopt_deref" name="opt_deref"> 
           <option value="LDAP_DEREF_NEVER" selected="selected">No</option> 
           <option value="LDAP_DEREF_ALWAYS">Si</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Atributo de usuario" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'El atributo usado para nombrar/buscar usuarios. Normalmente *cn*.' | i18n %]",);
            [% END %]
        </label> 
        <input name="user_attribute" id="user_attribute" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Atributo de miembro" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Especificar el atributo para nombre de usuario, cuando los usuarios se integran en un grupo. Normalmente *miembro*' | i18n %]",);
            [% END %]
        </label> 
        <input name="memberattribute" id=memberattribute" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Atributo de miembroutilizan dn" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Opcional: Anula el manejo de valores de atributos de los miembros, es 0 o 1' | i18n %]",);
            [% END %]
        </label> 
        <input name="memberattribute_isdn" id="memberattribute_isdn" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Clase de objetos" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Filtro usado para usuarios name/search. Normalmente debera ajustarlo a algo parecido a objectClass=posixAccount. Valores por defecto para objectClass=* que devolveran todos los objetos desde LDAP.' | i18n %]",);
            [% END %]
        </label> 
        <input name="objectclass" id="objectclass" size="30" type="text"> 
    </li>
    <br>
    <h4>Forzar cambio de contraseña </h4> 
    <li class="sub_item">
        <label>[% "Forzar cambio de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Forzar a los usuarios a cambiar la contraseña la primera vez que accedan a MERAN.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuforcechangepassword" class="select menuforcechangepassword" name="forcechangepassword"> 
           <option value="0" selected="selected">No</option> 
           <option value="1">Si</option> 
        </select>  
    </li>
    <li class="sub_item">
        <label>[% "Utilizar P&aacute;gina de cambio de contrase&ntilde;a est&aacute;ndar" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Si el sistema de autenticaci&oacute;n externa permite cambios de contrase&ntilde;a en
MERAN, seleccione Si. Este ajuste pasa por alto *Cambiar contraseña URL*. NOTA: Se recomienda que utilice LDAP sobre un t&uacute;nel encriptado SSL (ldaps://) si el servidor LDAP es remoto' | i18n %]",);
            [% END %]
        </label> 
        <select id="menustdchangepassword" class="select menustdchangepassword" name="stdchangepassword"> 
           <option value="0" selected="selected">No</option> 
           <option value="1">Si</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Formato de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Especifique el formato de las contrase&ntilde;as nuevas o cambiadas en el servidor LDAP' | i18n %]",);
            [% END %]
        </label> 
        <select id="menupasstype" class="select menupasstype" name="passtype"> 
           <option value="plaintext" selected="selected">Texto plano</option> 
           <option value="md5">Encriptaci&oacute;n M5</option> 
           <option value="sha1">SHA-1 hash</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "URL para cambio de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Aqui puede especificar donde pueden sus usuarios recuperar o cambiar su nombre de usuario/contrase&ntilde;a si lo han olvidado. Para ello, aparecera un boton en la pagina de entrada. Si deja esto en blanco, este boton no se mostrara.' | i18n %]",);
            [% END %]
        </label> 
        <input name="changepasswordurl" id="changepasswordurl" type="text"> 
    </li>
    <br>
    <h4>Ajustes de caducidad de la contraseña LDAP. </h4> 
    <li class="sub_item">
        <label>[% "Formato de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Seleccione No para deshabilitar comprobar si la contraseña ha caducado, o LDAP para leer el tiempo de caducidad de la contrase&ntilde;a directamente de LDAP.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuexpiration" class="select menuexpiration" name="expiration"> 
           <option selected="selected" value="0">no</option> 
           <option value="1">LDAP</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Advertencia de expiraci&oacute;n" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Numero de dias antes de que aparezca la advertencia de caducidad de la contrase&ntilde;a.' | i18n %]",);
            [% END %]
        </label> 
        <input name="expiration_warning" id="expiration_warning" size="2" value="10" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Atributo de expiraci&oacute;n" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Opcional: anula el atributo ldap que almacena el tiempo de caducidad de la contraseña PasswordExpirationTime.' | i18n %]",);
            [% END %]
        </label> 
        <input name="expireattr" id="expireattr" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "Formato de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Activar el soporte de entrada libre LDAP. Una ves que la contrase&ntilde;a ha caducado, el usuario puede entrar hasta que la cuenta de acceso libre llega a cero. Si se activa esta opci&oacute;n se mostrara un mensaje de acceso libre en el caso de que la contrase&ntilde;a haya caducado.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menugracelogins" class="select menugracelogins" name="gracelogins"> 
           <option selected="selected" value="0">No</option> 
           <option value="1">Si</option> 
        </select> 
    </li>
    <li class="sub_item">
        <label>[% "Atributo de entrada libre" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Opcional: Anula el atributo gracelogin.' | i18n %]",);
            [% END %]
        </label> 
        <input name="graceattr" id="graceattr" size="30" type="text"> 
    </li>
    <br>
    <h4>Habilitar creacion por parte del usuario </h4>
    <li class="sub_item">
        <label>[% "Formato de contrase&ntilde;a" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Los nuevos usuarios (an&oacute;nimos) pueden crear cuentas de usuario sobre el c&oacute;digo externo de autentificaci&oacute;n y confirmar v&iacute;a correo electr&oacute;nico. Si usted habilita esto, recuerde tambi&eacute;n configurar las opciones del m&oacute;dulo especifico para la creaci&oacute;n de usuario.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuauth_user_create" class="select menuauth_user_create" name="auth_user_create"> 
           <option selected="selected" value="0">No</option> 
           <option value="1">Si</option> 
        </select>  
    </li>    
    <li class="sub_item">
        <label>[% "Contexto para nuevos usuarios" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Si habilita la creaci&oacute;n de usuario con confirmaci&oacute;n por medio de correo electr&oacute;nico, especifique el contexto en el que se crean los usuarios. Este contexto debe ser diferente de otros usuarios para prevenir problemas de seguridad. No es necesario a&ntilde;adir este contexto a ldap_context-variable, MERAN buscara automaticamente los usuarios de este contexto.' | i18n %]",);
            [% END %]
        </label> 
        <input name="create_context" id="create_context" size="30" type="text"> 
    </li>
    <br>
    <h4>Autores de curso </h4> 
    <li class="sub_item">
        <label>[% "Creadores" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Lista de grupos cuyos miembros estan autorizados para crear nuevos cursos. Pueden separarse varios grupos con *;*. Normalmente asi: *cn=teachers,ou=staff,o=myorg*' | i18n %]",);
            [% END %]
        </label> 
        <input name="creators" id="creators" size="30" type="text">
    </li>
    <br>
    <h4>Script de sincronizaci&oacute;n del Cron </h4> 
    <li class="sub_item">
        <label>[% "Usuario externo eliminado" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Especifique que hacer con una cuenta de usuario interna durante sincronizaci&oacute;n masiva cuando el usuario fue eliminado de la fuente externa. Unicamente los usuarios suspendidos son automaticamente revividos si ellos reaparecen en una fuente externa' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuremoveuser" class="select menuremoveuser" name="removeuser"> 
           <option value="0" selected="selected">Mantener interna</option> 
           <option value="1">Suspender interna</option> 
           <option value="2">Borrado completo</option> 
        </select>  
    </li>
    <br>
    <h4>NTLM SSO </h4> 
    <li class="sub_item">
        <label>[% "Habilitar" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Seleccione Si para intentar Single Sign On con el dominio NTLM. Nota: esto requiere un ajuste adicional en el servidor web para trabajar.' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuntlmsso_enabled" class="select menuntlmsso_enabled" name="ntlmsso_enabled"> 
           <option value="0" selected="selected">No</option> 
           <option value="1">Si</option> 
        </select>  
    </li>
    <li class="sub_item">
        <label>[% "Sub-red" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Si se selecciona, solo intentar&aacute; el SSO con clientes de esta sub-red. Formato: xxx.xxx.xxx.xxx/bitmask' | i18n %]",);
            [% END %]
        </label> 
        <input name="ntlmsso_subnet" id="ntlmsso_subnet" size="30" type="text"> 
    </li>
    <li class="sub_item">
        <label>[% "MS IE fast path?" | i18n %]
            [% PERL %]
                print C4::AR::Filtros::setHelpIcon(text => "[% 'Set to yes to enable the NTLM SSO fast path (bypasses certain steps and only works if the clients browser is MS Internet Explorer). ' | i18n %]",);
            [% END %]
        </label> 
        <select id="menuntlmsso_ie_fastpath" class="select menuntlmsso_ie_fastpath" name="ntlmsso_ie_fastpath"> 
           <option value="0" selected="selected">No</option> 
           <option value="1">Si</option> 
        </select>  
    </li>

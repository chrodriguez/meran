<script>
if (parent.koha_menu.document.getElementById('vis').value=="yes")
	{document.getElementById('simenu').style.display='none';
	document.getElementById('nomenu').style.display='block';
	}else{
	document.getElementById('simenu').style.display='block';
	document.getElementById('nomenu').style.display='none';
	}

function colapse()
{
parent.document.getElementById('frameTop').cols='0,*';
document.getElementById('simenu').style.display='block';
document.getElementById('nomenu').style.display='none';
parent.koha_menu.document.getElementById('vis').value="no";
}
function expand(){
parent.document.getElementById('frameTop').cols='215,*';
document.getElementById('simenu').style.display='none';
document.getElementById('nomenu').style.display='block';
parent.koha_menu.document.getElementById('vis').value="yes";
}
</script>

